public class TesteCliente2 {
		public static void main(String args[]){
			Cliente2 c = new Cliente2();
			
			c.alterarCliente();
			
			//Exemplo chamada a um metodos com retorno
			int numero = 0;
			numero = c.obterNumeroCliente();
			
			//Exemplo chamada a um metodo com retorno e paramentro
			numero = c.obterNumeroCliente(0);
			
			//Exemplo chamada a um metodo estatico
			Conexao1.obterConexao();
			
		}
}
